{
  "name": "xr-archaeology",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "preinstall": "yarn config set ignore-engines true",
    "dev": "npm run watch:css && next dev",
    "build": "npm run watch:css && next build",
    "start": "next start",
    "lint": "next lint",
    "watch:css": "postcss styles/tailwind.css -o styles/main.css",
    "devMongo": "mkdir -p data && npx kill-port 27017 && mongod --dbpath data --replSet rs0",
    "docker:start": "docker-compose up",
    "docker:build": "docker-compose up --build",
    "docker:force": "docker-compose up --build --force-recreate --renew-anon-volumes",
    "server:start": "ts-node -r tsconfig-paths/register server/index.ts",
    "admin": "INIT_ADMIN=1 ts-node -r tsconfig-paths/register server/index.ts",
    "public": "ssh -R tests-server:80:localhost:3002 -R admin-tests-server:80:localhost:3001 serveo.net -R apsap-tests-server:80:localhost:3000"
  },
  "dependencies": {
    "@feathersjs/authentication": "^5.0.21",
    "@feathersjs/authentication-local": "^5.0.21",
    "@feathersjs/express": "^5.0.14",
    "@feathersjs/feathers": "^5.0.14",
    "@feathersjs/rest-client": "^5.0.14",
    "@feathersjs/socketio": "^5.0.14",
    "@feathersjs/socketio-client": "^5.0.14",
    "@feathersjs/transport-commons": "^5.0.21",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "bson": "^6.2.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "express": "^4.18.2",
    "feathers-hooks-common": "^8.1.2",
    "feathers-mongoose": "^8.5.1",
    "i18next": "^23.9.0",
    "i18next-http-backend": "^2.5.0",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "mongodb": "^6.3.0",
    "mongoose": "^8.0.4",
    "multer": "^1.4.5-lts.1",
    "next": "14.0.4",
    "react": "^18",
    "react-dom": "^18",
    "react-i18next": "^14.0.5",
    "react-icons": "^4.12.0",
    "sharp": "^0.33.3",
    "socket.io": "^4.7.3",
    "socket.io-client": "^4.7.4",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.202",
    "@types/mongodb": "^4.0.7",
    "@types/mongoose": "^5.11.97",
    "@types/multer": "^1.4.11",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/webpack-env": "^1.18.4",
    "autoprefixer": "^10.4.16",
    "dotenv": "^16.4.5",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "fs": "^0.0.1-security",
    "os": "^0.1.2",
    "postcss": "^8",
    "postcss-cli": "^11.0.0",
    "postcss-import": "^16.0.0",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5",
    "webpack": "^5.89.0"
  }
}
